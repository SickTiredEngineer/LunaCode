<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.itwillbs.luna_code.mapper.ClassSessionMapper">

    <insert id="insertClassSession" parameterType="ClassSession" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO class_session (
        class_id, 
        title
    ) VALUES (
    	#{classId}, 
    	#{title}
    )
    </insert>

</mapper>